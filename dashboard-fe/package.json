      "name": "Jack Herrington",
      "email": "jherr@pobox.com",
      "url": "http://jackherrington.com"
    }
  ],
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@nivo/chord": "^0.61.1",
    "apollo-boost": "^0.4.8",
    "apollo-server-micro": "^2.13.0",
    "clsx": "^1.1.0",
    "cross-fetch": "^3.0.4",
    "d3": "^5.16.0",
    "nedb": "^1.8.0",
    "next": "9.4.0",
