{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        "@babel/plugin-proposal-optional-chaining",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        "@babel/plugin-proposal-optional-chaining",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    }
  }
}
