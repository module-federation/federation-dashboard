{
  "name": "@dashboard/home",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@module-federation/dashboard-plugin": "file:../../dashboard-plugin",
    "babel-loader": "8.1.0",
    "css-loader": "3.6.0",
    "html-webpack-plugin": "git://github.com/ScriptedAlchemy/html-webpack-plugin#master",
    "less-loader": "6.1.2",
    "serve": "11.3.2",
    "style-loader": "1.2.1",
    "webpack": "5.0.0-beta.17",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development --liveReload",
    "cut-version": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\\\",]//g' | tr -d '[[:space:]]');mkdir dist/$PACKAGE_VERSION; cp -a dist/remoteEntry.js dist/$PACKAGE_VERSION/remoteEntry.js && node ./report-version.js",
    "build": "node ../../node_modules/webpack/bin/webpack.js --mode production && yarn cut-version",
    "serve": "serve dist -p 3001",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "antd": "^4.2.5",
    "lodash": "^4.17.15",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  }
}
