{
  "name": "@dashboard/utils",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@module-federation/dashboard-plugin": "*",
    "babel-loader": "8.1.0",
    "serve": "11.3.2",
    "webpack": "5.1.3",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development --liveReload",
    "cut-version": "node node_modules/.bin/federation-report --package-json=./package.json --build-dir=./dist",
    "build": "node ../../node_modules/webpack/bin/webpack.js --mode production && yarn cut-version",
    "serve": "serve dist -p 3005",
    "clean": "rm -rf dist"
  },
  "dependencies": {}
}
